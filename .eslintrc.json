{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "XMLHttpRequest": true,
        "FormData": true,
        "alert": true,
        "document": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "func-names": "off",
        "new-cap": "off",
        "no-plusplus": "off",
        "no-alert": "off",
        "no-use-before-define": "off",
        "consistent-return": "off",
        "no-console": "off"
    }
}